<div class="grid-container">
  <div>
    <div>
      <h1>Contracts</h1>
    </div>

    <table class="table table-sortable">
      <thead>
        <tr>
          <th>Contract_id</th>
          <th>Contract Title</th>
          <th>Terms/Agreement</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i < data.contracts.length; i++) { %>
          <tr>
            <td><%= data.contracts[i].contractid %></td>
            <td><%= data.contracts[i].title %></td>
            <td><%= data.contracts[i].terms %></td>
          </tr>
       <% } %>
      </tbody>
    </table>
  </div>

  <div>
    <div>
      <h1>Parties</h1>
    </div>

    <table class="table table-sortable">
      <thead>
        <tr>
          <th>Person 1</th>
          <th>Person 2</th>
          <th>Contact_id</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i < data.contracts.length; i++) { %>
          <tr>
            <td><%= data.contracts[i].parties.person1 %></td>
            <td><%= data.contracts[i].parties.person2 %></td>
            <td><%= data.contracts[i].contractid %></td>
          </tr>
       <% } %>
      </tbody>
    </table>
  </div>
</div>