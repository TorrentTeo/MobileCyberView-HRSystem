<div class="grid-container">

  <div>
    <div>
      <hr>
      <br>
      <h1>Feedback</h1>
      <br>
    </div>

    <table class="table table-sortable">
      <thead>
        <tr>
          <th>Regarding</th>
          <th>Content</th>
          <th>Submitted by</th>
          <th>Date</th>
          <th>Time</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i < data.feedbacks.length; i++) { %>
          <tr>
            <td><%= data.feedbacks[i].regarding %></td>
            <td style="width: 700px;"><%= data.feedbacks[i].content %></td>
            <td><%= data.feedbacks[i].name %></td>
            <td><%= data.feedbacks[i].date %></td>
            <td><%= data.feedbacks[i].time %></td>
          </tr>
       <% } %>
      </tbody>
    </table>
    <hr>
  </div>